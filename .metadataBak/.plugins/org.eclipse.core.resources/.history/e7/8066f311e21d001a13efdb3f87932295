	private static int[] threeShufflesBefore;

	public static void main(String[] args) throws IOException {
		Scanner in = new Scanner(new FileReader("stuff"));
		PrintWriter out = new PrintWriter(new File("stuff"));
		
		N = in.nextInt();
		shufflePattern = new int[N];
		finalOrder = new int[N];
		oneShufflesBefore = new int[N];
		twoShufflesBefore = new int[N];
		threeShufflesBefore = new int[N];
		for (int i = 0; i < N; i++) {
			shufflePattern[i] = in.nextInt();
		}
		for (int i = 0; i < N; i++) {
			finalOrder[i] = in.nextInt();
		}
		
		for (int i = 0; i < N; i++) {
			
		}

	}

}
