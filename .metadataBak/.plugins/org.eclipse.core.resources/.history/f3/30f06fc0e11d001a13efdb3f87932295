package stuff;

import java.util.*;
import java.io.*;

public class BovineShuffle {
	
	private static int N;
	private static int[] shufflePattern;
	private static int[] finalOrder;

	public static void main(String[] args) throws IOException {
		Scanner in = new Scanner(new FileReader("stuff"));
		PrintWriter out = new PrintWriter(new File("stuff"));
		
		N = in.nextInt();
		shufflePattern = new int[N];
		finalOrder = new int[N];
		for (int i = 0; i < N; i++) {
			shufflePattern[i] = in.nextInt();
			
		}

	}

}
